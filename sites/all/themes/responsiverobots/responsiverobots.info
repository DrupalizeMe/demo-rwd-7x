name = Responsive Robots
description = A mobile-friendly adaptation of the 960 Robots theme used as a demonstration in the Drupalize.Me video tutorial series Building and Theming a Mobile-Friendly Website in Drupal
core = 7.x

; These are standard, required regions
regions[help] = Help
regions[page_top] = Page Top
regions[page_bottom] = Page Bottom

; These are the regions we want for our layout
regions[header] = Header
regions[sidebar] = Sidebar
regions[content] = Content
regions[footer_left] = Footer left
regions[footer_right] = Footer right
regions[footer] = Footer bottom

; CSS stylesheets
stylesheets[all][] = css/reset.css
stylesheets[all][] = css/960.css
stylesheets[all][] = css/main.css

; JavaScript files
scripts[] = js/jquery.autofill.js
scripts[] = js/javascript.js

; Theme settings defaults.
settings[breadcrumb_delimiter] = ' Â» '
